<GuiControl
    Name="ToolboxDialog"
    Profile="GuiToolboxProfile"
    HorizSizing="right"
    VertSizing="bottom"
    Position="0 0"
    Extent="1024 768"
    MinExtent="320 320"
    Visible="1">
	
	<!-- Close Toolbox -->
    <GuiControl
		Name="TBCancelContainer"
        Profile="GuiDefaultProfile"
        VertSizing="relative"
		HorizSizing="relative"
        Position="928 672"
        Extent="96 96">

        <GuiImageButtonCtrl
            Name="ToolBoxCancelButton"
            command="toggleToolbox(true);"
            canSaveDynamicFields="0"
            isContainer="0"
            Profile="GuiDefaultProfile"
            HorizSizing="center"
            VertSizing="center"
            Position="0 0"
            Extent="96 96"
            MinExtent="8 2"
            canSave="1"
            Visible="1"
            Active="1"
            hovertime="1000"
            groupNum="-1"
            buttonType="PushButton"
            useMouseEvents="1"
            NormalImage="@asset=Sandbox:backBtnNormal"
            HoverImage="@asset=Sandbox:backBtnHover"
            DownImage="@asset=Sandbox:backBtnDown" />
    </GuiControl>
	
